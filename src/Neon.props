<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros" />
  <PropertyGroup>
    <OutDir>$(SolutionDir)\bin\$(PlatformShortName)\$(Configuration)\</OutDir>
    <IntDir>$(SolutionDir)\tmp\$(ProjectName)\$(PlatformShortName)\$(Configuration)\</IntDir>
    <IncludePath>$(SolutionDir)\src\;$(SolutionDir)\ref\inc\;$(IncludePath)</IncludePath>
    <LibraryPath>$(SolutionDir)\ref\lib\;$(SolutionDir)\ref\lib\$(PlatformShortName)\;$(SolutionDir)\ref\lib\$(PlatformShortName)\$(Configuration)\;$(SolutionDir)\bin\$(PlatformShortName)\$(Configuration)\;$(LibraryPath)</LibraryPath>
    <ExecutablePath>$(ExecutablePath)</ExecutablePath>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <Link>
      <AdditionalDependencies>%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>